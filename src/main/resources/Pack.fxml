<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.BoxBlur?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.final_project.CardSimController">
   <children>
      <ImageView fx:id="backgroundBlurred" fitHeight="400.0" fitWidth="612.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@startscreenbackground.jpg" />
         </image>
         <effect>
            <BoxBlur />
         </effect>
      </ImageView>
      <Button fx:id="openPackButton" layoutX="88.0" layoutY="169.0" mnemonicParsing="false" onAction="#changeScene2" prefHeight="50.0" prefWidth="150.0" text="Open A Pack">
         <font>
            <Font name="HP Simplified" size="12.0" />
         </font>
      </Button>
      <Button fx:id="MakeCardButton" layoutX="359.0" layoutY="169.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Make Your Own Card">
         <font>
            <Font name="HP Simplified" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
